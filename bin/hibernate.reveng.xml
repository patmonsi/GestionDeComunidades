<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-catalog="PUBLIC" match-schema="PUBLIC"
		match-name="COMUNIDADES" />
	<table-filter match-catalog="PUBLIC" match-schema="PUBLIC"
		match-name="INMUEBLES" />
	<table-filter match-catalog="PUBLIC" match-schema="PUBLIC"
		match-name="PROPIETARIOS" />
	<table catalog="PUBLIC" schema="PUBLIC" name="COMUNIDADES" class="dominio.Comunidad"></table>
	<table catalog="PUBLIC" schema="PUBLIC" name="PROPIETARIOS" class="dominio.Propietario"></table>
	<table catalog="PUBLIC" schema="PUBLIC" name="INMUEBLES" class="dominio.Inmueble"></table>
</hibernate-reverse-engineering>